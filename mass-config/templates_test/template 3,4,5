                <script>
                    //CSRF protection for AJAX requests
                    $.ajaxSetup({
                        headers: {
                            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                        }
                    });

                    device_id = 143;
                    embeddedVersion = 'Scoop version 4.8';
                    //device['version'] has a format like 'Scoop version 4.0'. Get only the version value (ex: '4.0').
                    const regexVersion = /([\d\.]+)$/g;
                    var matchResult = embeddedVersion.match(regexVersion);
                    embeddedVersion = matchResult ? matchResult[0] : null;

                    displayVariables(true);
                    $("#tbodyVariables").tooltip({ selector: '[data-toggle="tooltip"]' });

                    function displayVariables(firstLoading = null) {
                        //This function displays the variables according to the selected version
                        //On arrival on page, form'header is displayed
                        var deviceVersion = firstLoading ? null : $("select").val();
                        var admin = 1;
                        $.ajax({
                            type: 'POST',
                            url: 'ajax_form.php',
                            data: {
                                type: "refresh-config-version",
                                admin: admin,
                                deviceVersion: deviceVersion,
                                firstLoading: firstLoading,
                                device_id: device_id,
                            },
                            dataType: "json",
                            success: function (data) {
                                dataVersion = data.dataVersion;
                                if (firstLoading) {
                                    $("#configFormHeader").html(data.configFormHeader);
                                    customConfig = data.customConfig ? JSON.parse(data.customConfig) : null;
                                    deviceVersion = data.deviceVersion;
                                }
                                /*This fade in effect saves time to complete html.
                                Otherwise, overLoadFunction works to fast and can't select html elements.*/
                                $("#tbodyVariables").fadeOut(200, function () {
                                    $("#tbodyVariables").html(data.allVariables);
                                    $("#tbodyVariables").fadeIn(800);
                                });
                                if (customConfig) {
                                    setTimeout(function () {
                                        overLoadFunction(deviceVersion);
                                    }, 500);
                                }

                                /*If the page is refreshed and the selected version is lower than the device version,
                                show warning message. Without the fistLoading condition, the message is displayed each
                                time when user selects another version (without saving).*/
                                if (firstLoading && embeddedVersion && (deviceVersion < embeddedVersion)) {
                                    $("#alertInfo").addClass("btn btn-danger").html("");
                                    $("#alertInfo").append('La version demandée est inférieure à la version actuelle du boitier. <br/>' +
                                        'Un retour usine est attendu. <br/>');
                                } else if ($("#alertInfo").hasClass("btn btn-danger")) {
                                    $("#alertInfo").html("").removeClass("btn btn-danger");
                                }
                            },
                            error: function () {
                                $("#tbodyVariables").html("<h1 class='text-center'>Impossible to load variables</h1>");
                            }
                        });
                    }

                    function overLoadFunction(deviceVersion = ($("#selectVersion").val())) {
                        //this function change default values with custom values
                        $("#configForm")[0].reset();
                        $("#selectVersion").val(deviceVersion);

                        if ($("#alertInfo").hasClass("btn btn-danger") && (deviceVersion >= embeddedVersion)) {
                            $("#alertInfo").html("").removeClass("btn btn-danger");
                        }
                        if (customConfig) {
                            $.each(customConfig, function (variable, value) {
                                var fieldOverload = $("[name='" + variable + "']");

                                fieldOverload.val(value);
                                fieldOverload.closest('tr').addClass("overloadValue");
                            });
                        }
                    }

                    $("#tbodyVariables").on('click', '[class*="blocActions"] a', function () {
                        var idAction = $(this).attr("id");
                        if (idAction.includes("edit")) {
                            editAction(idAction);
                        }
                        else if (idAction.includes("suppr")) {
                            supprAction(idAction);
                        }
                        else if (idAction.includes("submit")) {
                            submitAction(idAction);
                        }
                    });

                    function editAction(idAction) {
                        //select which field
                        var idAction = idAction.replace("edit-", "");
                        var fieldAction = $("[name ='" + idAction + "']");
                        //field style for complete
                        $("[id='submit-" + idAction + "']").parent(".blocActionsSubmit").css("display", "inline-block");
                        $("[id='submit-" + idAction + "" + "']").parent(".blocActionsSubmit").css("display", "inline-block");
                        valFieldAction = fieldAction.val();

                        //Dynamic style when field is in "edit" mode.
                        fieldAction
                            .css({ "pointer-events": "auto", "border": "2px inset #EBE9ED" })
                            .focus()
                            .closest('tr').addClass("overloadValue");

                        if (fieldAction.is('input[type=number]') || fieldAction.is('select')) {
                            //Display arrows for select choice and for changing integer value (increase or decrease).
                            fieldAction.css({ "-webkit-appearance": "auto", "-moz-appearance": "auto" });
                        }

                        fieldAction.focusout(function () {
                            submitAction(idAction);
                        });
                    }

                    function submitAction(idAction) {
                        //select which field
                        var idAction = idAction.replace("submit-", "");
                        var fieldAction = $("[name ='" + idAction + "']");

                        fieldAction.css({ "pointer-events": "none", "border": "none" });
                        $("[id='submit-" + idAction + "']").parent(".blocActionsSubmit").css("display", "none");

                        //Removing dropdown arrows.
                        if (fieldAction.is('select')) {
                            fieldAction.css({ "-webkit-appearance": "none", "-moz-appearance": "none" });
                        }
                        else if (fieldAction.is('input[type=number]')) {
                            fieldAction.css({ "-webkit-appearance": "none", "-moz-appearance": "textfield" });
                        }
                    }

                    function supprAction(idAction) {
                        //select which field and put default value
                        var idAction = idAction.replace("suppr-", "");
                        var fieldAction = $("[name ='" + idAction + "']");

                        var group = idAction.substr(0, idAction.indexOf('.'));
                        var variable = idAction.substr(idAction.indexOf('.') + 1);
                        fieldAction.val(dataVersion[group][variable].defaultValue);

                        //Put default value style
                        fieldAction.closest('tr').removeClass("overloadValue");
                    }

                    //Click on "Save document"
                    $("#configFormHeader").on('click', "[name='btn-update-config-var']", function (event) {
                        event.preventDefault();
                        var newCustomConfig = customConfigFunction();
                        updateConfig(newCustomConfig);
                    });

                    function customConfigFunction() {
                        /*This function creates the custom config
                        after comparing form values and default values*/
                        var formData = $("form").serializeArray();
                        var customInProgress = {};
                        $.each(formData, function (key, field) {
                            var group = field.name.substr(0, field.name.indexOf('.'));
                            var variable = field.name.substr(field.name.indexOf('.') + 1);
                            if (group.length > 0 &&
                                ((dataVersion[group][variable].defaultValue) != field.value)) {
                                customInProgress[field.name] = field.value;
                            }
                        });
                        if ($.isEmptyObject(customInProgress)) {
                            return null;
                        }
                        //Convert to string
                        return JSON.stringify(customInProgress);
                    }

                    //By default, the previous versions are disabled.
                    function disabledDowngrade() {
                        $("#configFormHeader option").each(function (index, option) {
                            //If the selected version is higher than the option value : disabled.
                            if (this.value < $("#configFormHeader select").val()) {
                                $(this).attr("disabled", 'disabled');
                            } else {
                                $(this).attr("disabled", false);
                            }
                        });
                    }

                    //The user wants to downgrade. Show the warning modal.
                    $("#configFormHeader").on('click', '#btnDowngrade', function () {
                        $("#downgradeWarning").modal("show");
                    });

                    //The user confirms to downgrade. Access to previous versions.
                    function confirmDowngrade() {
                        $("#downgradeWarning").modal("hide");
                        $("#configFormHeader option").each(function (index, option) {
                            $(this).attr("disabled", false);
                        });
                    }

                    function updateConfig(newCustomConfig) {
                        var deviceVersion = $("select").val();
                        $.ajax({
                            type: 'POST',
                            url: 'ajax_form.php',
                            data: {
                                type: "update-config-values",
                                config: newCustomConfig,
                                device_id: device_id,
                                deviceVersion: deviceVersion
                            },
                            dataType: "json",
                            success: function (data) {
                                if (data.error_format.length) {
                                    //Display saved values and error messages
                                    $("tr").removeClass("overloadValue");
                                    overLoadFunction(deviceVersion);
                                    $("#alertInfo").addClass("btn btn-danger");
                                    $.each(data.error_format, function (key, error) {
                                        $("#alertInfo").append(error + ' <br/>');
                                    });
                                } else {
                                    toastr.success("Mise à jour effectuée.");
                                    customConfig = JSON.parse(newCustomConfig);
                                    $("#lastModification").html(data.lastModif);
                                    //When the new version is saved, disable the previous versions.
                                    disabledDowngrade();

                                    /*When user updates the configuration, if the selected version is lower
                                    than the device version, a warning message is displayed.*/
                                    if (embeddedVersion && deviceVersion < embeddedVersion) {
                                        $("#alertInfo").addClass("btn btn-danger").html("");
                                        $("#alertInfo").append('La version demandée est inférieure à la version actuelle du boitier. <br/>' +
                                            'Un retour usine est attendu. <br/>');
                                    } else if ($("#alertInfo").hasClass("btn btn-danger")) {
                                        $("#alertInfo").html("").removeClass("btn btn-danger");
                                    }
                                }
                            },
                            error: function () {
                                toastr.error('Error');
                            }
                        });
                    }

                </script>


            </div>
            <script type="text/javascript">
                document.title = 'fr01diro090f6641';
            </script>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {

        $("#countdown_timer_status").html("<i class=\"fa fa-pause fa-fw fa-lg\"></i> Pause");
        var Countdown = {
            sec: 300,

            Start: function () {
                var cur = this;
                this.interval = setInterval(function () {
                    $("#countdown_timer_status").html("<i class=\"fa fa-pause fa-fw fa-lg\"></i> Pause");
                    cur.sec -= 1;
                    display_time = cur.sec;
                    if (display_time == 0) {
                        location.reload();
                    }
                    if (display_time % 1 === 0 && display_time <= 300) {
                        $("#countdown_timer").html("<i class=\"fa fa-clock-o fa-fw fa-lg\"></i> Refresh in " + display_time);
                    }
                }, 1000);
            },

            Pause: function () {
                clearInterval(this.interval);
                $("#countdown_timer_status").html("<i class=\"fa fa-play fa-fw fa-lg\"></i> Resume");
                delete this.interval;
            },

            Resume: function () {
                if (!this.interval) this.Start();
            }
        };

        Countdown.Start();

        $("#countdown_timer_status").click("", function (event) {
            event.preventDefault();
            if (Countdown.interval) {
                Countdown.Pause();
            } else {
                Countdown.Resume();
            }
        });

        $("#countdown_timer").click("", function (event) {
            event.preventDefault();
        });

    });
</script>



<script type="text/javascript"></script>

<script>
    // start-larapoke-script
    if (typeof larapoke_script === 'undefined') {

        let larapoke_date = new Date();

        const larapoke_script = () => {
            let ajax = new XMLHttpRequest;

            ajax.onreadystatechange = () => {
                if (ajax.readyState === 4 && ajax.status === 204) {
                    larapoke_date = new Date();
                }
                larapoke_script_expired();
            };

            ajax.open('HEAD', 'http://scoop-test.yogoko.fr/poke');
            ajax.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
            ajax.send();

        };

        const larapoke_script_expired = () => {
            if (navigator.onLine && new Date() - larapoke_date >= 1800000 + 7200000) {
                location.reload(true);
            }
        };

        setInterval(() => { larapoke_script(); }, 1800000);

        document.addEventListener('visibilitychange', () => {
            if (!document.hidden) { larapoke_script_expired(); }
        }, false);

        window.addEventListener('online', larapoke_script_expired(), false);
    }
      // end-larapoke-script
</script>